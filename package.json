{
  "name": "nap",
  "version": "0.1.0",
  "scripts": {
    "lint": "eslint .",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "serve": "nodemon --harmony index.js",
    "build-front": "next build && npm run start",
    "build-back": "next build && npm run serve",
    "front": "npm run exec -- npm run dev",
    "back": "npm run exec -- npm run serve",
    "bi": "docker build -t rabbotio/nap . || true",
    "dive": "docker exec -it $(docker ps -a -q --filter ancestor=rabbotio/nap) /bin/sh || true",
    "exec": "docker exec -it $(docker ps -a -q --filter ancestor=rabbotio/nap) ",
    "rmi": "docker rmi -f rabbotio/nap || true",
    "clean": "docker rm -f $(docker ps -a -q --filter ancestor=rabbotio/nap) || true",
    "run": "docker run -p 3000:3000 -p 5858:5858 -d rabbotio/nap || true",
    "logs": "docker logs $(docker ps -a -q --filter ancestor=rabbotio/nap) || true",
    "stop": "docker stop $(docker ps -a -q --filter ancestor=rabbotio/nap) || true",
    "clear": "docker rm $(docker ps -aq) -f || true docker rmi -f rabbotio/nap && docker system prune -f && docker images && docker ps -a || true",
    "down": "docker-compose kill || true && docker-compose down || true",
    "up": "docker-compose build && docker-compose up -d && docker ps -a && docker attach $(docker ps -a -q --filter ancestor=rabbotio/nap) || true"
  },
  "dependencies": {
    "@risingstack/graffiti": "^3.3.0",
    "@risingstack/graffiti-mongoose": "^6.0.0",
    "apollo-client": "^0.8.5",
    "body-parser": "^1.16.1",
    "connect-redis": "^3.2.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "graphql": "^0.9.1",
    "graphql-tag": "^1.2.4",
    "graphql-tools": "^0.10.0",
    "isomorphic-fetch": "^2.2.1",
    "mongoose": "^4.8.2",
    "morgan": "^1.8.1",
    "next": "^2.0.0-beta",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-github2": "^0.1.10",
    "passport-google-oauth": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "react-apollo": "^0.10.1",
    "redux": "^3.6.0"
  },
  "author": "katopz@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.15.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "nodemon": "^1.11.0"
  }
}
